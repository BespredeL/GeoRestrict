{
    "name": "bespredel/geo-restrict",
    "description": "Restrict access to your Laravel app based on geolocation data.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Kireev",
            "email": "hello@bespredel.name",
            "homepage": "https://bespredel.name",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bespredel\\GeoRestrict\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bespredel\\GeoRestrict\\GeoRestrictServiceProvider"
            ]
        }
