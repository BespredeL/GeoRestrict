{
    "name": "bespredel/geo-restrict",
    "description": "Middleware to restrict access to Laravel applications based on IP geolocation.",
    "keywords": [
        "laravel",
        "middleware",
        "geoip",
        "ip",
        "geo",
        "location",
        "restriction",
        "access"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Kireev",
            "email": "hello@bespredel.name",
            "homepage": "https://bespredel.name",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bespredel\\GeoRestrict\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bespredel\\GeoRestrict\\GeoRestrictServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10|^11"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}